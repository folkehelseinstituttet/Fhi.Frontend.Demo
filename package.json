{
  "name": "fhi-frontend-library-web-client",
  "version": "0.11.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "npm run install-projects && ng build -c=production",
    "build-dev": "npm run build-libs && ng build -c=dev",
    "build-libs": "npm run build-angular-components && npm run build-angular-highcharts",
    "build-angular-components": "ng build @folkehelseinstituttet/angular-components",
    "build-angular-highcharts": "ng build @folkehelseinstituttet/angular-highcharts",
    "build-angular-components-watch": "ng build @folkehelseinstituttet/angular-components --watch",
    "build-angular-highcharts-watch": "ng build @folkehelseinstituttet/angular-highcharts --watch",
    "install-projects": "npm install --no-save @folkehelseinstituttet/angular-components @folkehelseinstituttet/angular-highcharts",
    "generate-icon-list": "node ./Fhi.Frontend.Style/src/fhi/icons/generate-icon-map && node ./src/MOCK_DB_DATA/library-items/icons/generate-icon-list",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "engines": {
    "node": "18"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.2",
    "@angular/cdk": "^18.2.0",
    "@angular/common": "^19.1.2",
    "@angular/compiler": "^19.1.2",
    "@angular/core": "^19.1.2",
    "@angular/forms": "^19.1.2",
    "@angular/platform-browser": "^19.1.2",
    "@angular/platform-browser-dynamic": "^19.1.2",
    "@angular/router": "^19.1.2",
    "@folkehelseinstituttet/style": "^6.0.0",
    "@ng-bootstrap/ng-bootstrap": "^18.0.0",
    "@ng-select/ng-select": "^14.2.0",
    "@popperjs/core": "^2.11.8",
    "@types/lodash-es": "~4.17.12",
    "bootstrap": "5.3.2",
    "date-fns": ">=3.3.1 <4",
    "highcharts": "^11.0.0",
    "highcharts-angular": "~4.0.1",
    "lodash-es": "~4.17.21",
    "ngx-markdown": "^19.0.0",
    "rxjs": "^7.8.0",
    "tslib": "^2.6.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.3",
    "@angular-eslint/builder": "^19.0.0",
    "@angular-eslint/eslint-plugin": "^19.0.0",
    "@angular-eslint/eslint-plugin-template": "^19.0.0",
    "@angular-eslint/schematics": "^19.0.0",
    "@angular-eslint/template-parser": "^19.0.0",
    "@angular/cli": "^19.1.3",
    "@angular/compiler-cli": "^19.1.2",
    "@types/jasmine": "^5.1.4",
    "@types/jasminewd2": "~2.0.13",
    "@types/node": "^22.10.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-prettier": "^5.2.0",
    "jasmine-core": "^5.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-packagr": "^19.1.0",
    "prettier": "^3.4.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.7.0",
    "webpack": "~5.97.0"
