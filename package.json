{
  "name": "fhi-frontend-library-web-client",
  "version": "0.11.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "npm run install-projects && npm run build-local-libs-for-prod && ng build -c=production",
    "build-dev": "npm run build-libs && ng build -c=dev",
    "build-libs": "npm run build-angular-components && npm run build-angular-highcharts && npm run build-angular-tree-view-builder",
    "build-local-libs-for-prod": "npm run build-angular-tree-view-builder",
    "build-angular-components": "ng build @folkehelseinstituttet/angular-components",
    "build-angular-highcharts": "ng build @folkehelseinstituttet/angular-highcharts",
    "build-angular-tree-view-builder": "ng build @folkehelseinstituttet/angular-tree-view-builder",
    "build-angular-components-watch": "ng build @folkehelseinstituttet/angular-components --watch",
    "build-angular-highcharts-watch": "ng build @folkehelseinstituttet/angular-highcharts --watch",
    "build-angular-tree-view-builder-watch": "ng build @folkehelseinstituttet/angular-tree-view-builder --watch",
    "install-projects": "npm install --no-save @folkehelseinstituttet/angular-components @folkehelseinstituttet/angular-highcharts",
    "generate-icon-list": "node ./Fhi.Frontend.Style/src/fhi/icons/generate-icon-map && node ./src/MOCK_DB_DATA/library-items/icons/generate-icon-list",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.2.2",
    "@angular/cdk": "^15.2.2",
    "@angular/common": "15.2.2",
    "@angular/core": "15.2.2",
    "@angular/forms": "15.2.2",
    "@angular/localize": "15.2.2",
    "@angular/platform-browser": "15.2.2",
    "@angular/platform-browser-dynamic": "15.2.2",
    "@angular/router": "15.2.2",
    "@circlon/angular-tree-component": "^11.0.4",
    "@folkehelseinstituttet/style": "5.6.0",
    "@ng-bootstrap/ng-bootstrap": "^14.0.0",
    "@ng-select/ng-select": "^10.0.3",
    "@popperjs/core": "^2.11.6",
    "@types/lodash-es": "~4.17.6",
    "angular-in-memory-web-api": "^0.15.0",
    "bootstrap": "5.2.3",
    "date-fns": "~2.29.0",
    "highcharts": "~11.1.0",
    "highcharts-angular": "~3.1.2",
    "lodash-es": "~4.17.21",
    "ngx-filter-pipe": "~2.1.2",
    "ngx-markdown": "~15.1.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.2",
    "@angular/cli": "^15.2.2",
    "@angular/compiler": "15.2.2",
    "@angular/compiler-cli": "15.2.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.3.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^15.2.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "4.9.5",
    "webpack": "5.0.0"
  }
}
