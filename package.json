{
  "name": "fhi-frontend-library-web-client",
  "version": "0.11.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "npm run install-projects && npm run build-local-libs-for-prod && ng build -c=production",
    "build-dev": "npm run build-libs && ng build -c=dev",
    "build-libs": "npm run build-angular-components && npm run build-angular-highcharts && npm run build-angular-tree-view-builder",
    "build-local-libs-for-prod": "npm run build-angular-tree-view-builder",
    "build-angular-components": "ng build @folkehelseinstituttet/angular-components",
    "build-angular-highcharts": "ng build @folkehelseinstituttet/angular-highcharts",
    "build-angular-tree-view-builder": "ng build @folkehelseinstituttet/angular-tree-view-builder",
    "build-angular-components-watch": "ng build @folkehelseinstituttet/angular-components --watch",
    "build-angular-highcharts-watch": "ng build @folkehelseinstituttet/angular-highcharts --watch",
    "build-angular-tree-view-builder-watch": "ng build @folkehelseinstituttet/angular-tree-view-builder --watch",
    "install-projects": "npm install --no-save @folkehelseinstituttet/angular-components @folkehelseinstituttet/angular-highcharts",
    "generate-icon-list": "node ./Fhi.Frontend.Style/src/fhi/icons/generate-icon-map && node ./src/MOCK_DB_DATA/library-items/icons/generate-icon-list",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@ali-hm/angular-tree-component": "^12.0.0",
    "@angular/animations": "16.2.8",
    "@angular/cdk": "16.2.7",
    "@angular/common": "16.2.8",
    "@angular/core": "16.2.8",
    "@angular/forms": "16.2.8",
    "@angular/localize": "16.2.8",
    "@angular/platform-browser": "16.2.8",
    "@angular/platform-browser-dynamic": "16.2.8",
    "@angular/router": "16.2.8",
    "@folkehelseinstituttet/style": "5.6.0",
    "@ng-bootstrap/ng-bootstrap": "^15.0.0",
    "@ng-select/ng-select": "^11.0.0",
    "@popperjs/core": "^2.11.6",
    "@types/lodash-es": "~4.17.6",
    "angular-in-memory-web-api": "^0.16.0",
    "bootstrap": "5.2.3",
    "date-fns": "^2.30.0",
    "highcharts": "~11.1.0",
    "highcharts-angular": "~3.1.2",
    "lodash-es": "~4.17.21",
    "ngx-markdown": "~16.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.6",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "^16.2.6",
    "@angular/compiler": "16.2.8",
    "@angular/compiler-cli": "16.2.8",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jasmine-core": "~4.3.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^16.2.3",
    "prettier": "^3.0.3",
    "ts-node": "~8.3.0",
    "typescript": "4.9.5",
    "webpack": "^5.88.2"
  }
}
