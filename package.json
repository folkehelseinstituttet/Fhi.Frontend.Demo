{
  "name": "fhi-frontend-library-web-client",
  "version": "0.11.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "npm run install-projects && npm run build-local-libs-for-prod && ng build -c=production",
    "build-dev": "npm run build-libs && ng build -c=dev",
    "build-libs": "npm run build-angular-components && npm run build-angular-highcharts && npm run build-angular-tree-view-builder",
    "build-local-libs-for-prod": "npm run build-angular-tree-view-builder",
    "build-angular-components": "ng build @folkehelseinstituttet/angular-components",
    "build-angular-highcharts": "ng build @folkehelseinstituttet/angular-highcharts",
    "build-angular-tree-view-builder": "ng build @folkehelseinstituttet/angular-tree-view-builder",
    "build-angular-components-watch": "ng build @folkehelseinstituttet/angular-components --watch",
    "build-angular-highcharts-watch": "ng build @folkehelseinstituttet/angular-highcharts --watch",
    "build-angular-tree-view-builder-watch": "ng build @folkehelseinstituttet/angular-tree-view-builder --watch",
    "install-projects": "npm install --no-save @folkehelseinstituttet/angular-components @folkehelseinstituttet/angular-highcharts",
    "generate-icon-list": "node ./Fhi.Frontend.Style/src/fhi/icons/generate-icon-map && node ./src/MOCK_DB_DATA/library-items/icons/generate-icon-list",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "engines": {
    "node": "18"
  },
  "private": true,
  "dependencies": {
    "@ali-hm/angular-tree-component": "^12.0.0",
    "@angular/animations": "17.3.3",
    "@angular/cdk": "17.0.5",
    "@angular/common": "17.3.3",
    "@angular/core": "17.3.3",
    "@angular/forms": "17.3.3",
    "@angular/localize": "17.3.3",
    "@angular/platform-browser": "17.3.3",
    "@angular/platform-browser-dynamic": "17.3.3",
    "@angular/router": "17.3.3",
    "@folkehelseinstituttet/style": "6.1.1",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@ng-select/ng-select": "^12.0.6",
    "@popperjs/core": "^2.11.8",
    "@types/lodash-es": "~4.17.6",
    "angular-in-memory-web-api": "^0.17.0",
    "bootstrap": "5.3.2",
    "date-fns": ">=3.3.1 <4",
    "highcharts": "^11.1.0",
    "highcharts-angular": "~4.0.0",
    "lodash-es": "~4.17.21",
    "ngx-markdown": "^17.1.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.3",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "17.3.3",
    "@angular/compiler": "17.3.3",
    "@angular/compiler-cli": "17.3.3",
    "@types/jasmine": "^5.1.4",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^20.11.1",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jasmine-core": "^5.1.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^17.0.3",
    "prettier": "^3.0.3",
    "ts-node": "~8.3.0",
    "typescript": "5.2.2",
    "webpack": "^5.88.2"
  }
}
