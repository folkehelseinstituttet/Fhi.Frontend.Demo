{
  "name": "fhi-frontend-library-web-client",
  "version": "0.11.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "npm run install-projects && ng build -c=production",
    "build-dev": "npm run build-libs && ng build -c=dev",
    "build-libs": "npm run build-angular-components && npm run build-angular-highcharts",
    "build-angular-components": "ng build @folkehelseinstituttet/angular-components",
    "build-angular-highcharts": "ng build @folkehelseinstituttet/angular-highcharts",
    "build-angular-components-watch": "ng build @folkehelseinstituttet/angular-components --watch",
    "build-angular-highcharts-watch": "ng build @folkehelseinstituttet/angular-highcharts --watch",
    "install-projects": "npm install --no-save @folkehelseinstituttet/angular-components @folkehelseinstituttet/angular-highcharts",
    "generate-icon-list": "node ./Fhi.Frontend.Style/src/fhi/icons/generate-icon-map && node ./src/MOCK_DB_DATA/library-items/icons/generate-icon-list",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "engines": {
    "node": "18"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.2.4",
    "@angular/cdk": "18.2.3",
    "@angular/common": "18.2.4",
    "@angular/core": "18.2.4",
    "@angular/forms": "18.2.4",
    "@angular/localize": "18.2.4",
    "@angular/platform-browser": "18.2.4",
    "@angular/platform-browser-dynamic": "18.2.4",
    "@angular/router": "18.2.4",
    "@folkehelseinstituttet/style": "^6.0.0",
    "@ng-bootstrap/ng-bootstrap": "^17.0.1",
    "@ng-select/ng-select": "^13.7.1",
    "@popperjs/core": "^2.11.8",
    "@types/lodash-es": "~4.17.6",
    "bootstrap": "5.3.2",
    "date-fns": ">=3.3.1 <4",
    "highcharts": "~11.4.0",
    "highcharts-angular": "~4.0.0",
    "lodash-es": "~4.17.21",
    "ngx-markdown": "^18.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~18.2.4",
    "@angular-eslint/builder": "18.3.1",
    "@angular-eslint/eslint-plugin": "18.3.1",
    "@angular-eslint/eslint-plugin-template": "18.3.1",
    "@angular-eslint/schematics": "18.3.1",
    "@angular-eslint/template-parser": "18.3.1",
    "@angular/cli": "18.2.4",
    "@angular/compiler": "18.2.4",
    "@angular/compiler-cli": "18.2.4",
    "@types/jasmine": "^5.1.4",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^20.11.1",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jasmine-core": "^5.1.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^18.2.1",
    "prettier": "^3.0.3",
    "ts-node": "~8.3.0",
    "typescript": "5.5.4",
